{
  "name": "bscheshirwork/yii2-socketio",
  "description": "The simple and powerful socketio for the Yii2 framework",
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexey Orlov",
      "email": "aaorlov88@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "yiisoft/yii2": "2.*",
    "yiicod/yii2-cron": "1.*",
    "yiicod/yii2-base": "1.*",
    "symfony/process": "~3.0|~4.0|~5.0|~6.0|~7.0",
    "predis/predis": "~1.1.0"
  },
  "require-dev": {
    "symplify/easy-coding-standard": "^12.0",
    "rector/rector": "^0.18.12"
  },
  "scripts": {
    "ecs": "vendor/bin/ecs check",
    "ecs-fix": "vendor/bin/ecs check --fix",
    "rector": "vendor/bin/rector process --dry-run",
    "rector-fix": "vendor/bin/rector process"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "bscheshirwork\\socketio\\": "/"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": false
    }
  }
}
